            <string>//Draw the GUI
if (global.ending = 0)
{
//window variables
width = window_get_width();
height = window_get_height();


//Part 1: Draw the line splitting the p1 and p2 views
draw_set_color(c_black);
draw_line_width(width/2, 0, width/2, height, 3);

//Part 2: Draw the box with the Timer in it
//for now we'll say it's 200 x 100

//draw the white box first
draw_set_color(c_white);
draw_roundrect((width/2) - 100, height - 200, (width/2) + 100, height - 100, false);

//draw the text
draw_set_color(c_black);
draw_set_font(fnt_numbers);
draw_set_halign(fa_center);
draw_text(width/2, height - 150, obj_alphaControl.seconds);
}

//draw player 1 and player 2 health bars
//player 1
draw_set_color(c_red);
draw_rectangle(0,height-64,(obj_playerOne.myhealth/100)*500,height,false);
//player 2
draw_rectangle(width,height-64,width-((obj_playerTwo.myhealth/100)*500),height,false);

//draw their stats
//attack
draw_set_color(c_red);
draw_rectangle(0,0,obj_playerOne.myatt*40,20,false);
draw_rectangle(width,0,width-(obj_playerTwo.myatt*40),20,false);

//defense
draw_set_color(c_blue);
draw_rectangle(0,20,obj_playerOne.mydef*40,40,false);
draw_rectangle(width,20,width-(obj_playerTwo.mydef*40),40,false);

//speed
draw_set_color(c_green);
draw_rectangle(0,40,obj_playerOne.myspeed*40,60,false);
draw_rectangle(width,40,width-(obj_playerTwo.myspeed*40),60,false);
</string>
